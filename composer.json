{
  "name": "archangeldesign/php-gson",
  "description": "PHP GSON equivalent - simple entity mapper. JSON to object and object to JSON",
  "license": "BSD-3-Clause",
  "type": "library",
  "keywords": [
    "gson",
    "entity",
    "mapper"
  ],
  "version": "1.1",
  "authors": [
    {
      "name": "Rafal Martinez-Marjanski"
    }
  ],
  "homepage": "https://archangeldesign.github.io/php-gson/",
  "autoload": {
    "psr-0": {
      "PHPGson" : "src/"
    }
  },
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^8"
  },
  "suggest": {
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev",
      "dev-develop": "1.1-dev"
    }
  }
}
